#include <iostream>
using namespace std;

// Function to check if a number is prime
bool isPrime(int n) {
    if (n <= 1) return false;
    if (n == 2) return true;
    if (n % 2 == 0) return false;

    for (int i = 3; i * i <= n; i += 2) {
        if (n % i == 0)
            return false;
    }
    return true;
}

int main() {
    int lower, upper;
    cout << "Enter lower and upper limits: ";
    cin >> lower >> upper;

    cout << "Prime numbers between " << lower << " and " << upper << " are:\n";
    for (int i = lower; i <= upper; i++) {
        if (isPrime(i))
            cout << i << " ";
    }
    cout << endl;
    return 0;
}
