#include <iostream>
using namespace std;

int main() {
    int n = 5; // Number of lines

    for (int i = 1; i <= n; i++) {  // Loop for each row
        for (int j = 0; j < i; j++) {  // Loop to print characters
            cout << char('A' + j);
        }
        cout << endl;  // Move to next line
    }

    return 0;
}
